{
  "name": "@nomicfoundation/hardhat-node-test-coverage",
  "version": "3.0.0-next.4",
  "description": "A node:test coverage collector for Hardhat",
  "homepage": "https://github.com/nomicfoundation/hardhat/tree/v-next/v-next/hardhat-node-test-coverage",
  "repository": {
    "type": "git",
    "url": "https://github.com/NomicFoundation/hardhat",
    "directory": "v-next/hardhat-node-test-coverage"
  },
  "author": "Nomic Foundation",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/src/collector.js"
  },
  "keywords": [
    "ethereum",
    "smart-contracts",
    "hardhat"
  ],
  "scripts": {
    "lint": "pnpm prettier --check && pnpm eslint",
    "lint:fix": "pnpm prettier --write && pnpm eslint --fix",
    "eslint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier \"**/*.{ts,js,md,json}\"",
    "test": "node --import tsx/esm --test \"test/**/*.ts\"",
    "test:only": "node --import tsx/esm --test --test-only \"test/**/*.ts\"",
    "test:coverage": "c8 --reporter html --reporter text --all --exclude test --exclude \"src/**/{types,type-extensions}.ts\" --src src node --import tsx/esm --test \"test/**/*.ts\"",
    "pretest": "pnpm build",
    "pretest:only": "pnpm build",
    "build": "tsc --build .",
    "prepublishOnly": "pnpm build",
    "clean": "rimraf dist"
  },
  "files": [
    "dist/src/",
    "src/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "c8": "^9.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-no-only-tests": "3.1.0",
    "expect-type": "^0.19.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.19.3",
    "typescript": "~5.5.0",
    "typescript-eslint": "7.7.1"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-errors": "workspace:3.0.0-next.4",
    "@nomicfoundation/hardhat-utils": "workspace:3.0.0-next.4"
  },
  "peerDependencies": {
    "hardhat": "workspace:^3.0.0-next.4"
  }
}
