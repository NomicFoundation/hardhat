{
  "name": "@nomicfoundation/eslint-plugin-hardhat-internal-rules",
  "version": "1.0.2",
  "private": true,
  "description": "This plugin contains internal linting rules used in the [Hardhat repository](https://github.com/nomiclabs/hardhat/). It's not meant to be used by other projects.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "license": "MIT",
  "author": "Nomic Foundation",
  "exports": "./lib/index.js",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.61.0",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "eslint": "^8.44.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
