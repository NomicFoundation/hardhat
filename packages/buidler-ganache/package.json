{
  "name": "@nomiclabs/buidler-ganache",
  "version": "1.1.2",
  "description": "Buidler plugin for managing Ganache",
  "homepage": "https://github.com/nomiclabs/buidler/tree/master/packages/buidler-ganache",
  "repository": "github:nomiclabs/buidler",
  "author": "Nomic Labs LLC",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "ethereum",
    "smart-contracts",
    "buidler",
    "buidler-plugin",
    "Ganache",
    "testing-network"
  ],
  "scripts": {
    "lint:fix": "cross-env ../../node_modules/.bin/prettier --write 'src/**/*.{js,ts}' 'test/**/*.{js,ts}' && npm run lint-src -- --fix && npm run lint-tests -- --fix",
    "lint": "npm run lint-src && npm run lint-tests",
    "lint-tests": "cross-env ../../node_modules/.bin/tslint --config tslint.json --project ./tsconfig.json",
    "lint-src": "cross-env ../../node_modules/.bin/tslint --config tslint.json --project src/tsconfig.json",
    "test": "./scripts/run-tests.sh",
    "build": "cross-env ../../node_modules/.bin/tsc --build src",
    "build-test": "cross-env ../../node_modules/.bin/tsc --build .",
    "clean": "rm -rf dist build-test"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "debug": "^4.1.1",
    "ganache-core": "^2.7.0",
    "ts-essentials": "^2.0.7",
    "ts-interface-checker": "^0.1.9"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.1.2",
    "@types/chai": "^4.2.0",
    "@types/debug": "^4.1.4",
    "@types/fs-extra": "^5.1.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "ts-interface-builder": "^0.2.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.5.3"
  },
  "peerDependencies": {
    "@nomiclabs/buidler": "^1.1.2"
  }
}
