#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kanej/projects/nomiclabs/hardhat/node_modules/.pnpm/eslint-doc-generator@1.7.1_eslint@8.57.0_typescript@5.7.2/node_modules/eslint-doc-generator/dist/bin/node_modules:/Users/kanej/projects/nomiclabs/hardhat/node_modules/.pnpm/eslint-doc-generator@1.7.1_eslint@8.57.0_typescript@5.7.2/node_modules/eslint-doc-generator/dist/node_modules:/Users/kanej/projects/nomiclabs/hardhat/node_modules/.pnpm/eslint-doc-generator@1.7.1_eslint@8.57.0_typescript@5.7.2/node_modules/eslint-doc-generator/node_modules:/Users/kanej/projects/nomiclabs/hardhat/node_modules/.pnpm/eslint-doc-generator@1.7.1_eslint@8.57.0_typescript@5.7.2/node_modules:/Users/kanej/projects/nomiclabs/hardhat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kanej/projects/nomiclabs/hardhat/node_modules/.pnpm/eslint-doc-generator@1.7.1_eslint@8.57.0_typescript@5.7.2/node_modules/eslint-doc-generator/dist/bin/node_modules:/Users/kanej/projects/nomiclabs/hardhat/node_modules/.pnpm/eslint-doc-generator@1.7.1_eslint@8.57.0_typescript@5.7.2/node_modules/eslint-doc-generator/dist/node_modules:/Users/kanej/projects/nomiclabs/hardhat/node_modules/.pnpm/eslint-doc-generator@1.7.1_eslint@8.57.0_typescript@5.7.2/node_modules/eslint-doc-generator/node_modules:/Users/kanej/projects/nomiclabs/hardhat/node_modules/.pnpm/eslint-doc-generator@1.7.1_eslint@8.57.0_typescript@5.7.2/node_modules:/Users/kanej/projects/nomiclabs/hardhat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint-doc-generator/dist/bin/eslint-doc-generator.js" "$@"
else
  exec node  "$basedir/../eslint-doc-generator/dist/bin/eslint-doc-generator.js" "$@"
fi
